<h1>Products</h1>
<div class="index">
  <%= link_to "New product", new_product_path, class: "button-new-product" %>

  <div class="product">
    <div id="products">
      <% @products.each do |product| %>
        <div class="sanpham">
          <%= link_to product.name, product %>
        </div>
      <% end %>
    </div>
  </div>
</div>